@model List<CoreFinalTest.Models.ChetuStudentModel>
@using Microsoft.AspNetCore.Http
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor

<h1>@HttpContextAccessor.HttpContext.Session.GetString("name")</h1>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-dark">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Password</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Mobile</td>
            <td>@item.Password</td>
            <td><a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id" class="btn btn-success">Edit</a></td>
            <td><a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id" class="btn btn-warning">Delete</a></td>
        </tr>
    }
</table>
